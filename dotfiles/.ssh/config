Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_ed25519_github.ssh
    ForwardAgent yes

Host pi
    HostName 192.168.0.80
    User root
    IdentityFile ~/.ssh/id_ed25519_pi

Host server
    HostName server
    User tefloon
    IdentityFile ~/.ssh/ed25519_server

Host hostinger
    HostName hostinger
    User root
    IdentityFile ~/.ssh/id_ed25519_hostinger 


Host *
    ControlMaster auto
    ControlPath ~/.ssh/cm-%r@%h:%p
    ControlPersist 10m